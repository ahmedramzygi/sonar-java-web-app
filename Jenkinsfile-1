pipeline {
  agent any  
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
         sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=ahmedramzygi_sonar-java-web-app_AYMIZLw3bNVeIO-b4L6v"
        }
      }
    }
  }
}
